{
	"Bot": {
		// 正向 WebSocket 地址
		"WSAddress": "ws://127.0.0.1:8888",
		// 管理员QQ
		"ManagerQQ" : 123456,
		// 是否发送错误消息
		"IsSendErrorMessage": true,
		// 超时撤回错误消息，0不撤回，单位秒
		"DeleteErrorMessageTimeout": 15
	},
	"Pixiv": {
		// 代理地址
		"Proxy": "http://127.0.0.1:1082",
		// 图片的反向代理地址，不填则会使用 proxy 下载图片
		// 如 https://i.pixiv.re 
		"PximgReverseProxy": "",
		// pixiv账号的cookie，如不填入，搜索的结果会有限制
		"Cookie": "",
		// 图片下载超时，超时会重试3次，单位秒
		"Timeout": 30,
		// 发送的图片数量
		"MaxImages": 6,
		// 图片质量 0-100，如大于等于 100 则发送原图
		"ImageQuality": 80
	},
	"Approve": {
		// 自动同意好友请求
		"AutoApproveFriendAdd": false,
		// 自动同意群邀请请求
		"AutoApproveGroupInvite": false,
	},
	"LlamaCpp": {
		// 服务地址
		"ServerAddress": "http://127.0.0.1:8088",
		// 记录消息日志
		"WriteMessageLog": false,
		// 使用群名片，启用后机器人和群员的名字为群名片
		"UseGroupCard": false,
		// 机器人名字
		"BotName": "gege",
		// 提示词
		"Prompt": "You are a helpful assistant. 你是一个乐于助人的助手。",
		// 模型对话参数，调整生成文本的随机性
		"Temperature": 0.8,
		// 最大记忆对话数
		"MemoryLimit": 20,
		// 记忆重置命令
		"ResetCommand": "重置记忆",
		// 重置后发送的消息，不填不发送
		"ResetMessage": "记忆已重置。",
		// 要过滤掉的文本，避免触发回复
		"FilterText": ["--重载配置", "搜"],
		// 字典文件集，用于自动回复
		"DictFile": ["dict.json"],
		// 自动回复概率 0.0 - 1.0
		"ReplyProbability": 0.5,
	},
}